#include <stdio.h>
#include <stdlib.h>

int main()
{
    int arr[5]= {10, 20, 30, 40, 50};
    int *ptr1 = &arr[0];
    int *ptr2 = &arr[3];


     if (ptr1 < ptr2) {
            printf ("ptr1 apunta a una direccion menor que ptr2\n");
        } else {
            printf ("ptr1 apunta a una direccion mayor o igual que ptr2\n");
        }

    return 0;
}

